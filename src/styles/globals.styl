@import '_variables'
@import '_mixins'

*
  box-sizing border-box

html
  height 100%

body
  font-family sans-serif
  line-height 1.4
  min-height 100%
  -moz-font-feature-settings: "liga", "kern";
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  text-rendering: optimizeLegibility;
  margin: 0
  padding: 0

  

dump_media_cache()
